<!DOCTYPE html>
<html lang="en">
<head>
  <title>Yesder</title>
  
</head>
<body>
<div class="container">
  <div class="panel-groupa">
    <div class="panel panel-info">
      <div class="panel-heading"><div class="labela"><h3>Log-in</h3></div></div>
      <div class="panel-body">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="labela"><label for="usr">User:</label>
    <%= f.label :email %><br />
    <div class="email"> <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
     </div>
    <%= f.label :password %><br />
   <div class="contrase">  <%= f.password_field :password, autocomplete: "off" %>
   </div>  

  <% if devise_mapping.rememberable? -%>
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
  
  <% end -%>
  
  <div class="actions">
    <%= f.submit "Log in" %></div>
  </div>
<% end %>
<%= render "devise/shared/links" %>


      </div>
    </div>
    </div>
    </div>

</body>
</html>